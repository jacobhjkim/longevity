{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [".env"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "inputs": [".env"]
    },
    "//#format": {},
    "//#lint": {},
    "//#lint:fix": {
      "cache": false
    }
  }
}
